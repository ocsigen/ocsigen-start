<?xml version='1.0' encoding='utf-8'?>
<widget id="com.%%%PROJECT_NAME%%%.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>%%%PROJECT_NAME%%%</name>
    <description>
      Eliom-base-app mobile application
    </description>
    <author email="developer@domain.com" href="http://domain.com">
        %%%PROJECT_NAME%%% team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <chcp>
        <config-file url="%%APPSERVER%%/update/chcp.json"/>
    </chcp>
</widget>
